{
  "name": "bs-layout-boilerplate-angular2",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server ./dist",
    "typings": "typings",
    "postinstall": "typings install",
    "cp:theme": "copyfiles -f node_modules/bootstrap-layout/dist/* node_modules/bootstrap/dist/js/bootstrap.min.js node_modules/bootstrap/dist/css/bootstrap.min.css* node_modules/jquery/dist/jquery.min.js node_modules/tether/dist/js/tether.min.js dist/vendor/theme",
    "cp:es6-shim": "ncp node_modules/es6-shim dist/vendor/es6-shim",
    "cp:angular2-in-memory-web-api": "ncp node_modules/angular2-in-memory-web-api dist/vendor/angular2-in-memory-web-api",
    "cp:systemjs": "ncp node_modules/systemjs dist/vendor/systemjs",
    "cp:rxjs": "ncp node_modules/rxjs dist/vendor/rxjs",
    "cp:reflect-metadata": "ncp node_modules/reflect-metadata dist/vendor/reflect-metadata",
    "cp:zonejs": "ncp node_modules/zone.js dist/vendor/zone.js",
    "cp:angular": "ncp node_modules/@angular dist/vendor/@angular",
    "cp": "concurrently \"npm run cp:theme\" \"npm run cp:es6-shim\" \"npm run cp:angular2-in-memory-web-api\" \"npm run cp:systemjs\" \"npm run cp:rxjs\" \"npm run cp:reflect-metadata\" \"npm run cp:zonejs\" \"npm run cp:angular\"",
    "deploy": "surge --project ./dist --domain bs-layout-boilerplate-angular2.themekit.io"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "4.0.0-alpha.2",
    "bootstrap-layout": "1.0.0-alpha.5",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "jquery": "^2.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "tether": "^1.2.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "copyfiles": "^0.2.1",
    "lite-server": "^2.1.0",
    "ncp": "^2.0.0",
    "surge": "^0.17.7",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
